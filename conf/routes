# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

# Negocios
GET     /negocios                   controllers.Application.getNegocios

#Get nuevo negocio
GET 	/nuevoNegocio				controllers.Application.nuevoNegocio

#Agregar negocio
POST	/agregarNegocio				controllers.Application.agregarNegocio

# Precios
GET     /precios                   controllers.Application.getPreciosRegistrados

#Get nuevo negocio
GET 	/nuevoPrecioRegistrado				controllers.Application.nuevoPrecioRegistrado

#Agregar negocio
POST	/agregarPrecioRegistrado				controllers.Application.agregarPrecioRegistrado



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
